<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialRipple">
        <attr name="ripple_alpha" format="integer" />
        <attr name="ripple_framerate" format="integer"/>
        <attr name="ripple_rippleDuration" format="integer"/>
        <attr name="ripple_zoomDuration" format="integer" />
        <attr name="ripple_color" format="color" />
        <attr name="ripple_centered" format="boolean" />
        <attr name="ripple_type" format="enum">
            <enum name="simpleRipple" value="1"/>
            <enum name="doubleRipple" value="1"/>
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="ripple_ripplePadding" format="dimension" />
        <attr name="ripple_zoom" format="boolean" />
        <attr name="ripple_zoomScale" format="float" />

    </declare-styleable>
</resources>
